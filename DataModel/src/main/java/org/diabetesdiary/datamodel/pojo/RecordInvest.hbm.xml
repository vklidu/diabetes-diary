<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="record_investigation" name="org.diabetesdiary.datamodel.pojo.RecordInvest">
    <composite-id class="org.diabetesdiary.datamodel.pojo.RecordInvestPK" name="id">
      <key-property name="idInvest"/>
      <key-property name="idPatient"/>
      <key-property name="date"/>
    </composite-id>
    <property name="value" not-null="true" />
    <property name="season" not-null="true" />
    <property name="notice" />
    <many-to-one column="idInvest" name="invest" insert="false" update="false" lazy="false" />    
  </class>
</hibernate-mapping>
