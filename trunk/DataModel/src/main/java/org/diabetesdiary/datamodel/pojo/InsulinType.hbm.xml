<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="insulin_type" name="org.diabetesdiary.datamodel.pojo.InsulinType">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="name" not-null="true" />
    <property name="description"/>
    <property name="parameterA" not-null="true"/>
    <property name="parameterB" not-null="true"/>
    <property name="parameterS" not-null="true"/>
    <set table="insulin" lazy="false" inverse="true" cascade="all" name="insulins">
      <key column="idType"/>
      <one-to-many class="org.diabetesdiary.datamodel.pojo.Insulin"/>
    </set>
  </class>
</hibernate-mapping>
