<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="investigation" name="org.diabetesdiary.datamodel.pojo.Investigation">
        <id name="id">
            <generator class="native"/>
        </id>        
        <many-to-one name="group" column="idGroup" not-null="true" lazy="false"/>
        <property name="name" not-null="true" />
        <property name="unit" not-null="true" />
        <property name="normalMin"/>
        <property name="normalMax"/>
    </class>
</hibernate-mapping>
